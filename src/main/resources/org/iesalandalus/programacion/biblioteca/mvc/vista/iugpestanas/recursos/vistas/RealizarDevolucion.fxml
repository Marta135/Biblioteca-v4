            <Label alignment="CENTER" prefHeight="30.0" prefWidth="174.0" text="          Fecha de préstamo:">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lbFechaPrestamo" prefHeight="32.0" prefWidth="222.0">
