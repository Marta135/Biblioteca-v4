                  <Insets />
               </GridPane.margin>
            </Label>
            <Label alignment="CENTER" prefHeight="30.0" prefWidth="75.0" text="Alumno:" />
            <Label fx:id="lbAlumno" prefHeight="35.0" prefWidth="292.0" GridPane.columnIndex="1" />
