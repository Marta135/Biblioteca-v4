<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="272.0" prefWidth="437.0" stylesheets="@../estilos/estilos.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iesalandalus.programacion.biblioteca.mvc.vista.grafica.controladoresvistas.ControladorRealizarDevolucion">
   <children>
      <GridPane alignment="CENTER" hgap="10.0" prefHeight="87.0" prefWidth="397.0" vgap="10.0">
