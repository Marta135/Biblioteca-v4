                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="dpFechaDevolucion" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="67.0" prefWidth="437.0" spacing="30.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Button fx:id="btCancelar" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
            <Button fx:id="btAceptar" mnemonicParsing="false" onAction="#realizarDevolucion" text="Aceptar" />
         </children>
      </HBox>
